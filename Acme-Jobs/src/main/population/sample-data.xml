<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<import resource="conversion-service.xml" />

<bean id="shout1" class="acme.entities.shouts.Shout">
	<property name="author" value="John"  />
	<property name="text" value="Look this"  />
	<property name="moment" value="2019/08/09 15:55"/>
</bean>

<bean id="shout2" class="acme.entities.shouts.Shout">
	<property name="author" value="Mark"  />
	<property name="text" value="Look that"  />
	<property name="moment" value="2019/08/09 15:55"/>
</bean>

<bean id="shout3" class="acme.entities.shouts.Shout">
	<property name="author" value="Luke"  />
	<property name="text" value="Look those"  />
	<property name="moment" value="2019/08/09 15:55"/>
</bean>
</beans>



